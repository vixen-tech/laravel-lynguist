{
    "name": "vixen/laravel-lynguist",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Alex Torscho",
            "email": "contact@alextorscho.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^12.43"
    },
    "require-dev": {
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-laravel": "^4.0",
        "orchestra/testbench": "^10.8"
    },
    "autoload": {
        "psr-4": {
            "Vixen\\Lynguist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vixen\\Lynguist\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vixen\\Lynguist\\LynguistServiceProvider"
            ]
        }
    }
}
